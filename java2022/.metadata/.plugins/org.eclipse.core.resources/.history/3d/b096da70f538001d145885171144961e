package recapDemo1;

public class MultiDimensionalArrayDemo {

	public static void main(String[] args) {
		String[][] sehirler =new String[3][3];//Şehirler string olduğu için string yazıyoruz.
		//Daha sonra satır ve sütün oluşan bir sehir ve bölgeleri yazacagım için [] [] yazdım.
		//Biri satır biri sütuna it köşeli parantexlerin. 3 adet Şehirler 3 adet bölgeler yazacagım için [3] [3] yazıldı.
		//3*3 den 9 eleman olacak.
		
		sehirler[0][0] ="İstanbul";//Dizilerde ilk eleman 0 dan başlar .
		sehirler[0][1] ="Bursa";
		sehirler[0][2] ="Bilecik";
		sehirler[1][0] ="Ankara";
		sehirler[1][1] ="Konya";
		sehirler[1][2] ="Kayseri";
		sehirler[2][0] ="Diyarbakır";
		sehirler[2][1] ="Şanlıurfa";
		sehirler[2][2] ="Gaziantep";
		
		for(int i=0; i<=2; i++) {// İ inci elemanın j inci elemanına ulaşmak için i ve j için ayrı for döngüsü yazılır.
		System.out.println("*********");
			for(int j=0;j<=2;j++) {
				System.out.println(sehirler[i][j]);//şehirlerin i,j inci elemanını yazdır.
			}
		}
		

	}

}
