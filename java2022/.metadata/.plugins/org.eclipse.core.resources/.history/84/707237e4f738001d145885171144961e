package recapDemo1;

public class stringsDemo {

	public static void main(String[] args) {
		String mesaj="Bugün hava çok güzel";
		System.out.println(mesaj);
		
		System.out.println("eleman sayısı:" +mesaj.length());//mesaj daki tüm karakterleri say ve yazıdr.
        System.out.println("5.eleman:"+ mesaj.charAt(4));//mesaj daki 5.elemanı yazdır.1. eleman 0 oldugu için 5.eleman 4. eleman olarak sayılır.
	}

}
